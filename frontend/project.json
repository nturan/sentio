{
    "name": "frontend",
    "$schema": "../node_modules/nx/schemas/project-schema.json",
    "projectType": "application",
    "sourceRoot": "frontend/src",
    "targets": {
        "serve": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npm run dev",
                "cwd": "frontend"
            }
        },
        "build": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npm run build",
                "cwd": "frontend"
            }
        },
        "lint": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npm run lint",
                "cwd": "frontend"
            }
        }
    },
    "tags": []
}